<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.clubswaitress.cubswaitressapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            android:usesCleartextTraffic="true">
        <activity android:name="com.clubswaitress.cubswaitressapp.Pages.MenuClubActivity"></activity>
        <activity
                android:name="com.clubswaitress.cubswaitressapp.Pages.BillTransferActivity"
                android:configChanges="orientation|keyboardHidden|screenSize"
                android:label="@string/title_activity_bill_transfer" />
        <activity android:name="com.clubswaitress.cubswaitressapp.ActivityListener" />
        <activity android:name="com.clubswaitress.cubswaitressapp.Pages.BillActivity" />
        <activity android:name="com.clubswaitress.cubswaitressapp.Pages.HallwithTablesActivity" />
        <activity android:name="com.clubswaitress.cubswaitressapp.LoginActivity" />
        <activity
                android:name="com.clubswaitress.cubswaitressapp.MainActivity"
                android:label="@string/app_name"
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>